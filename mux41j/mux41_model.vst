entity mux41_model is
   port (
      x2  : in      bit;
      vss : in      bit;
      vdd : in      bit;
      x   : out     bit
 );
end mux41_model;

architecture structural of mux41_model is
Component buf_x8
   port (
      i   : in      bit;
      q   : out     bit;
      vdd : in      bit;
      vss : in      bit
 );
end component;


begin

x_ins : buf_x8
   port map (
      i   => x2,
      q   => x,
      vdd => vdd,
      vss => vss
   );


end structural;
